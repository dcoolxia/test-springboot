<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="name.nvshen.user.dao.UserMapper">

	<insert id="insertUser" parameterType="name.nvshen.user.bean.User">
		insert into t_user(name,level)
		values(#{name,jdbcType=NUMERIC}, #{level,jdbcType=INTEGER})
	</insert>

</mapper>